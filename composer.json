{
    "name": "manipulate-files",
    "description": "A Laravel Mediable Extension that enables of files manipulation",
    "keywords": ["mediable","laravel-mediable","manipulation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammed Manssour",
            "email": "manssour.mohammed@gmail.com"
        }
    ],
    "require": {
        "plank/laravel-mediable": "^2.5",
        "intervention/image" : "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.5"
    },
    "config": {
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "Missionx\\Mediable\\Manipulation\\": "src/"
        }
    },
    "autoload-dev":{
        "classmap": ["tests/"]
    }
}
